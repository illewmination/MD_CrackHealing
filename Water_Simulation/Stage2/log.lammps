#pair_coeff 1 4 1186.492877	0.297		0		#Ca-H
pair_style lj/cut/coul/cut 10.0
pair_modify tail yes
#kspace_style pppm 1.0e-5

pair_coeff 1 1 0.00000 0.000
pair_coeff 1 2 0.00000 0.000
pair_coeff 2 2 0.15535 3.166

#kspace_style	pppm 1.0e-5

bond_style	harmonic
#bond_coeff	1 20.424650 1.3042000	#C-O
bond_coeff	1 22.965000 1.0120000	#Ow-H

angle_style 	hybrid class2 harmonic
#angle_coeff	1 class2    120.00000 6.6170000 0.0000000 0.0000000
#angle_coeff	1 class2 bb 12.818000 1.3042000 1.3042000
#angle_coeff	1 class2 ba 1.5331900 1.5331900 1.3042000 1.3042000
angle_coeff	1 harmonic  1.6456800 113.24000 #water

#improper_style 	distance
#improper_coeff 	1 13.647000 360.00000

neighbor        2.0 bin
neigh_modify    every 1 delay 1 check yes one 4000

#Raiteri P, et al. Derivation of an accurate force-field for simulating the growth of calcium carbonate from aqueous solution: A new model for the calciteâˆ’ Water interface. The Journal of Physical Chemistry C 2010, 114.13: 5997-6010 (Pair_styles)

#Raiteri P, Demichelis R, Gale JD. Thermodynamically consistent force field for molecular dynamics simulations of alkaline-earth carbonates and their aqueous speciation. The Journal of Physical Chemistry C 2015, 119(43): 24447-24458 (Else)

#replicate	3 3 3

group		Ow type 1
7020 atoms in group Ow
group		H type 2
3510 atoms in group H

timestep	0.001
#minimize 	0.0 0.0 50000 200000

#------------------------------------------
# Initial cell length for strain calculations
variable tmpz equal "lz"
variable Lz0 equal ${tmpz}
variable Lz0 equal 203.875191369682
print "Initial Length, Lz0: ${Lz0}"
Initial Length, Lz0: 203.875191369682

variable hin equal bound(all,ymax)
variable fro equal bound(all,ymin)
variable tLy0 equal "v_hin-v_fro"
variable Ly0 equal ${tLy0}
variable Ly0 equal 56.4359384200485

variable wallh equal "v_hin-12"
variable wallf equal "v_fro+12"
region hind block INF INF ${wallh} INF INF INF
region hind block INF INF 44.1156763144708 INF INF INF
region fron block INF INF INF ${wallf} INF INF
region fron block INF INF INF 11.6797378944223 INF INF
group hind region hind
2270 atoms in group hind
group fron region fron
2247 atoms in group fron
group surface union hind fron
4517 atoms in group surface
#group surf_rig subtract surface O

compute 	peatom all pe/atom
compute 	keatom all ke/atom
compute		statom all stress/atom NULL
dump            1 all custom 500 dump.water id type x y z c_peatom c_keatom c_statom[1] c_statom[2] c_statom[3] c_statom[4] c_statom[5] c_statom[6]

# 1 bar = 1/10000 GPa
variable strain equal "(lz - v_Lz0)/v_Lz0"
variable sx equal "-pxx/10000"
variable sy equal "-pyy/10000"
variable sz equal "-pzz/10000"

thermo          50
thermo_style    custom step v_strain v_sx v_sy v_sz lx ly lz pe ke etotal press temp

# ------------------------- Equilibration-------------------------
reset_timestep	0
velocity        all create 300.0 1234568 dist uniform

#fix constrain all shake 1.0e-4 100 0 b 1 a 1
fix integrate all npt temp 300 300 0.2 iso 1 1 2
fix removeMomentum all momentum 1 linear 1 1 1
run 5000
